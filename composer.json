{
    "name": "bentools/url",
    "type": "library",
    "description": "Url object representation",
    "keywords": ["url", "uri", "object"],
    "homepage": "https://github.com/bpolaszek/bentools-url",
    "license": "MIT",
    "authors": [
        {
            "name": "Beno!t POLASZEK",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bpolaszek/bentools-url"
        }
    ],
    "require": {
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3"
    },
    "autoload": {
        "psr-4": {"BenTools\\Url\\": "src/"}
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    }
}
